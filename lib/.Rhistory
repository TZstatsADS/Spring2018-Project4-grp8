}
#agg_class(X, alpha, allPars)
agg_class <- function(X, alpha, allPars) {
n <- nrow(X)
B <- length(alpha)
label <- matrix(0, n, 1)
if (B == 1) {
allPars <- rbind(allPars, matrix(0,1,3))
}
for (b in seq(B)) {
label <- label + alpha[b] * classify(X, allPars[[b]])
}
c_hat <- sign(label)
return(c_hat)
}
